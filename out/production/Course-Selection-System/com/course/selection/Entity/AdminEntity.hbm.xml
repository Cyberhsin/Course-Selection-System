<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.course.selection.Entity.AdminEntity" table="admin" schema="course_selection">
        <id name="adminId">
            <column name="admin_id" sql-type="varchar(30)" length="30"/>
            <generator class="uuid"/>
        </id>
        <property name="adminName">
            <column name="admin_name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="adminPwd">
            <column name="admin_pwd" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>